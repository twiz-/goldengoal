<div class="page-header">
 <h1>All Notes</h1>
</div> 

<%= link_to "Make a New Note", new_note_path, class: "btn btn-success" %>

<% @notes.each do |note| %>
<div class="note">
 <strong><%= note.name %></strong>
 <p><%= note.comments %></p>
 <p><%= note.practice %></p>
 <div class="meta">
  <%= link_to time_ago_in_words(note.created_at) + " ago", note %>
  <span class="admin">
    | <%= link_to "Edit", edit_note_path(note) %> |
    <%= link_to "Delete",note, method: :delete, data: { confirm: "are you sure you want to delete note?"} %>
    
  </span>
 </div>
</div> 
<% end %>